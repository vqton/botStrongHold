[loggers]
keys=root,sampleLogger

[handlers]
keys=hand02

[formatters]
keys=sampleFormatter

[logger_root]
level=DEBUG
handlers=hand02

[logger_sampleLogger]
level=DEBUG
handlers=hand02
qualname=sampleLogger
propagate=0

[handler_consoleHandler]
class=StreamHandler
level=DEBUG
formatter=sampleFormatter
args=(sys.stdout,)

[formatter_sampleFormatter]
format=%(asctime)s - %(name)s - %(levelname)s - %(message)s

[handler_hand02]
class=FileHandler
level=DEBUG
formatter=sampleFormatter
args=('botStrongHold.log', 'w+')
